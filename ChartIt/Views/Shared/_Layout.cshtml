                if (monthsOld < 12)
                    return monthsOld + ' Months';
                else if (monthsOld < 24)
                    return '1 Year and ' + (monthsOld % 12) + ' Months';
                else
                    return Math.floor(monthsOld / 12) + ' Years and ' + (monthsOld % 12) + ' Months';
